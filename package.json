{
  "name": "smartfish",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "start:all": "node scripts/start-all.js",
    "dev:all": "node scripts/start-all.js",
    "python:server": "node scripts/start-python-server.js",
    "python:setup": "cd backend && python -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
    "apk:build": "node scripts/build-apk.js debug",
    "apk:build-release": "node scripts/build-apk.js release",
    "apk:init": "bubblewrap init --manifest=https://smartfishcare.site/manifest.json --config=twa-manifest.json",
    "apk:update": "bubblewrap update"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.22.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "next": "16.0.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.8",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "^3.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
